{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "name": "Debug Blinky",
            "servertype": "openocd",
            "gdbPath": "/usr/bin/gdb-multiarch",
            "cwd": "${workspaceRoot}",
            "device": "STM32H743xx",
            "svdFile": "./STM32H743.svd",
            "preLaunchTask": "cargo build blinky",
            "executable": "${workspaceFolder}/target/thumbv7em-none-eabihf/debug/blinky",
            "configFiles": [
                "./openocd.cfg"
            ],
            "runToMain": true,
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "name": "Debug ITM",
            "servertype": "openocd",
            "gdbPath": "/usr/bin/gdb-multiarch",
            "cwd": "${workspaceRoot}",
            "device": "STM32H743xx",
            "svdFile": "./STM32H743.svd",
            "preLaunchTask": "cargo build spi",
            "executable": "${workspaceFolder}/target/thumbv7em-none-eabihf/debug/itm",
            "configFiles": [
                "./openocd.cfg"
            ],
            "runToMain": true,
        }
    ]
}
